{
  "version": 2,
  "name": "adify",
  "public": true,
  "alias": ["adify"],
  "scope": "iqubex-technologies",
  "build": {
    "env": {
      "TEST": "@test"
    }
  },
  "builds": [{
    "src": "packages/web/package.json",
    "use": "@now/next"
  }],
  "routes": [{
    "src": "(.*)",
    "dest": "packages/web$1",
    "continue": true
  }]
}