{
  "target": "serverless",
  "version": 2,
  "name": "adify",
  "public": true,
  "alias": [
    "adify"
  ],
  "scope": "iqubex-technologies",
  "build": {
    "env": {

    }
  },
  "builds": [{
      "src": "web/src/server/index.js",
      "use": "@now/node-server"
    },
    {
      "src": "web/next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [{
    "src": "/(.*)",
    "dest": "/web/$1"
  }]
}